
public class Lvl2UniqueSum {

	public static void main(String[] args) {
		int int1 = 1;
		int int2 = 2;
		int int3 = 3;
		
		System.out.println(uniqueSum(int1, int2, int3));

	}
	
	public static int uniqueSum(int num1, int num2, int num3) {

	if (num1 - num2 = 0) {
		num1 = 0;
		num2 = 0;
	};
	if (num2 - num3 = 0) {
		num1 = 0;
		num2 = 0;
	};
	if (num3 - num1 = 0) {
		num1 = 0;
		num2 = 0;
	};
	
return num1 + num2 + num3;
	}
}
